{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"  
    },
    {
      "src": "/:path*",
      "dest": "/public/:path*",
      "exclude": ["/api/*", "/login_page.html"],  
      "continue": true,  
      "handler": "@vercel/static"
    }
  ]
}
