{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",  
        "dest": "/server.js"  
      },
      {
        "src": "/(.*)",   
        "dest": "/login_page.html",  
        "conditions": [
          "!**/*.css", 
          "!**/*.js",  
          "!**/*.png", 
          "!**/*.jpg"
        ]
      }
    ]
  }